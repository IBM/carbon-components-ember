{
  "name": "doc-app",
  "version": "0.0.0",
  "private": true,
  "description": "Doc app for carbon-components-ember addon",
  "repository": "https://github.com/IBM/carbon-components-ember",
  "license": "MIT",
  "author": "Patrick Pircher",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "vite build",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:glint": "glint",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "optimize": "vite optimize",
    "start": "vite",
    "debug:types": "glint --debug-intermediate-representation",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "build:ember": "EMBROIDER_PREBUILD=true ember b",
    "classic:ember-build": "bash ./scripts/prepare-classic.sh && EMBROIDER_TEST_SETUP_FORCE=classic ember build"
  },
  "devDependencies": {
    "@ascua/arrays": "^0.4.6",
    "@babel/eslint-parser": "^7.22.5",
    "@babel/plugin-proposal-decorators": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/plugin-transform-typescript": "^7.21.3",
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^4.0.4",
    "@embroider/compat": "3.7.1-unstable.b470496",
    "@embroider/config-meta-loader": "1.0.1-unstable.9f1e74e",
    "@embroider/core": "3.4.20-unstable.b470496",
    "@embroider/test-setup": "4.0.1-unstable.b470496",
    "@embroider/vite": "0.2.2-unstable.b470496",
    "@embroider/webpack": "^3.2.2",
    "@gavant/glint-template-types": "^0.4.0",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@glint/core": "^1.5.0",
    "@glint/environment-ember-loose": "^1.5.0",
    "@glint/environment-ember-template-imports": "^1.5.0",
    "@glint/template": "^1.5.0",
    "@nullvoxpopuli/ember-composable-helpers": "^5.2.5",
    "@rollup/plugin-babel": "^6.0.4",
    "carbon-components-ember": "workspace:*",
    "concurrently": "^9.1.2",
    "decorator-transforms": "^2.3.0",
    "ember-auto-import": "^2.10.0",
    "ember-cli": "~6.1.0",
    "ember-cli-addon-docs": "^7.2.2",
    "ember-cli-addon-docs-yuidoc": "^1.1.0",
    "ember-cli-app-version": "^7.0.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.3",
    "ember-cli-deploy": "^2.0.0",
    "ember-cli-deploy-build": "link:libs/ember-cli-deploy-build",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-data": "5.3.9",
    "ember-fetch": "^8.1.2",
    "ember-load-initializers": "^3.0.1",
    "ember-modifier": "^4.2.0",
    "ember-page-title": "^8.2.3",
    "ember-qunit": "^9.0.1",
    "ember-resolver": "^13.1.0",
    "ember-routable-component": "^1.2.0",
    "ember-source": "^6.1.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-svg-jar": "^2.6.0",
    "ember-template-lint": "^5.11.0",
    "ember-truth-helpers": "^4.0.3",
    "ember-try": "^3.0.0",
    "ember-vite-hmr": "^1.8.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-ember": "^12.1.1",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-qunit": "^8.1.1",
    "loader.js": "^4.7.0",
    "prettier": "^3.2.5",
    "prettier-plugin-ember-template-tag": "^2.0.1",
    "qunit": "^2.19.4",
    "qunit-dom": "^2.0.0",
    "stylelint": "^15.9.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-prettier": "^3.0.0",
    "tracked-built-ins": "^3.1.1",
    "typescript": "^5.4.5",
    "vite": "^5.1.6",
    "webpack": "^5.90.3"
  },
  "engines": {
    "node": "16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "type": "app",
    "version": 2
  },
  "exports": {
    "./tests/*": "./tests/*",
    "./*": "./app/*"
  }
}
