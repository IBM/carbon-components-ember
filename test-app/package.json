{
  "name": "test-app",
  "version": "0.0.0",
  "private": true,
  "description": "Test app for carbon-components-ember addon",
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint --allow-empty-input \"**/*.css\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:glint": "glint",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "start": "vite",
    "test": "pnpm test:ember",
    "test:all": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "node ./scripts/run-tests.mjs",
    "test:ember:update-snapshot": "node ./scripts/run-tests.mjs update-snapshots",
    "test:ember-dist": "vite build --mode test && ember test --path dist",
    "classic:test-ember": "ember test",
    "classic:build-ember": "EMBROIDER_TEST_SETUP_FORCE=classic ember build",
    "classic:ember-serve": "EMBROIDER_TEST_SETUP_FORCE=classic ember test --serv",
    "ember:serve": "vite  --mode=test --force --open tests/"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/eslint-parser": "^7.22.5",
    "@babel/plugin-proposal-decorators": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.27.3",
    "@babel/plugin-transform-typescript": "^7.27.1",
    "@babel/runtime": "^7.27.6",
    "@carbon/styles": "^1.87.0",
    "@carbon/themes": "^11.53.0",
    "@ember/optional-features": "^2.2.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^5.2.2",
    "@embroider/compat": "^4.1.0",
    "@embroider/config-meta-loader": "^1.0.0",
    "@embroider/core": "^4.1.1",
    "@embroider/macros": "^1.18.0",
    "@embroider/test-setup": "^4.0.0",
    "@embroider/vite": "^1.1.5",
    "@embroider/webpack": "^4.0.8",
    "@glimmer/component": "^2.0.0",
    "@glimmer/tracking": "^1.1.2",
    "@glint/core": "^1.5.2",
    "@glint/environment-ember-loose": "^1.5.0",
    "@glint/environment-ember-template-imports": "^1.5.0",
    "@glint/template": "^1.5.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@types/qunit": "^2.19.5",
    "astroturf": "^1.2.0",
    "babel-plugin-ember-template-compilation": "^3.0.0",
    "carbon-components-ember": "workspace:*",
    "concurrently": "^9.1.2",
    "decorator-transforms": "^2.2.2",
    "ember-auto-import": "^2.10.0",
    "ember-cli": "^6.1.0",
    "ember-cli-app-version": "^7.0.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-clean-css": "^2.0.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-htmlbars": "^6.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-load-initializers": "^3.0.1",
    "ember-modifier": "^4.2.2",
    "ember-page-title": "^9.0.2",
    "ember-qunit": "^9.0.3",
    "ember-resolver": "^13.0.2",
    "ember-resources": "^7.0.4",
    "ember-routable-component": "^1.2.0",
    "ember-source": "^6.4.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-imports": "^4.1.1",
    "ember-template-lint": "^5.11.0",
    "ember-try": "^4.0.0",
    "ember-vite-hmr": "^2.0.1",
    "eslint": "^9.27.0",
    "@eslint/js": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-ember": "^12.5.0",
    "eslint-plugin-n": "^17.18.0",
    "eslint-plugin-prettier": "^5.5.0",
    "eslint-plugin-qunit": "^8.1.2",
    "globals": "^16.2.0",
    "js-reporters": "^2.1.0",
    "loader.js": "^4.7.0",
    "prettier": "^3.3.2",
    "prettier-plugin-ember-template-tag": "^2.0.2",
    "puppeteer-chromium-resolver": "^24.0.1",
    "qunit": "^2.24.1",
    "qunit-dom": "^3.4.0",
    "stylelint": "^16.19.1",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-prettier": "^3.0.0",
    "tracked-built-ins": "^4.0.0",
    "vite": "^6.3.5",
    "webpack": "^5.99.5"
  },
  "engines": {
    "node": "16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "type": "app",
    "version": 2
  },
  "exports": {
    "./tests/*": "./tests/*",
    "./*": "./app/*"
  }
}
