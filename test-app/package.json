{
  "name": "test-app",
  "version": "0.0.0",
  "private": true,
  "description": "Test app for carbon-components-ember addon",
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint --allow-empty-input \"**/*.css\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:glint": "glint",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "start": "vite --mode=test",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "build:ember": "EMBROIDER_PREBUILD=true ember b",
    "test:ember": "vite build --mode test && ember test --path dist",
    "classic:test-ember": "ember test",
    "classic:build-ember": "EMBROIDER_TEST_SETUP_FORCE=classic ember build",
    "classic:ember-serve": "EMBROIDER_TEST_SETUP_FORCE=classic ember test --serv",
    "ember:serve": "vite  --mode=test --force --open tests/"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.5",
    "@babel/plugin-proposal-decorators": "^7.22.5",
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^3.3.0",
    "@embroider/compat": "unstable",
    "@embroider/macros": "^1.16.9",
    "@embroider/config-meta-loader": "unstable",
    "@embroider/core": "unstable",
    "@embroider/test-setup": "unstable",
    "@embroider/vite": "unstable",
    "@embroider/webpack": "^3.2.2",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@glint/core": "^1.5.0",
    "@glint/environment-ember-loose": "^1.5.0",
    "@glint/environment-ember-template-imports": "^1.5.0",
    "@glint/template": "^1.5.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@types/qunit": "^2.19.5",
    "carbon-components-ember": "workspace:*",
    "concurrently": "^8.2.0",
    "ember-auto-import": "^2.7.4",
    "ember-cli": "^5.10.0",
    "ember-cli-app-version": "^6.0.1",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-clean-css": "^2.0.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-htmlbars": "^6.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-load-initializers": "^3.0.1",
    "ember-modifier": "^4.1.0",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^8.1.0",
    "ember-resolver": "^13.0.2",
    "ember-source": "^5.10.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.11.0",
    "ember-try": "^3.0.0",
    "ember-vite-hmr": "^1.8.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-ember": "^12.1.1",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-qunit": "^8.1.1",
    "js-reporters": "^2.1.0",
    "loader.js": "^4.7.0",
    "prettier": "^3.3.2",
    "prettier-plugin-ember-template-tag": "^2.0.2",
    "puppeteer-chromium-resolver": "^22.0.0",
    "qunit": "^2.19.4",
    "qunit-dom": "^2.0.0",
    "stylelint": "^15.9.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-prettier": "^3.0.0",
    "tracked-built-ins": "^3.1.1",
    "webpack": "^5.90.3",
    "vite": "^5.4.9",
    "ember-routable-component": "^1.2.0",
    "ember-template-imports": "^4.1.1",
    "decorator-transforms": "^2.2.2"
  },
  "engines": {
    "node": "16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "type": "app",
    "version": 2
  },
  "exports": {
    "./tests/*": "./tests/*",
    "./*": "./app/*"
  }
}
