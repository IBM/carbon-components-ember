{{import onUpdate from '../../-helpers/on-update'}}
{{import toBool from '../../-helpers/to-bool'}}
{{import '{ fn }' from '@ember/helper'}}

{{#if @chart.setAxis}}
  {{onUpdate @axis @title @stacked (fn @chart.setAxis @axis (hash
    title=@title
    stacked=(toBool @stacked)
    primary=(toBool @primary)
    secondary=(toBool @secondary)
    scaleType=@scaleType
  ))}}
{{/if}}
