{{import Chart from '../-components/chart'}}
{{import set from 'carbon-components-ember/helpers/set'}}

<Chart
  {{! template-lint-disable  no-capital-arguments }}
  @ChartClass={{this.ChartClass}}
  @resizable={{@resizable}}
  @legendClickable={{@legendClickable}}
  ...attributes
  as |chart|
>
  {{yield chart}}
</Chart>