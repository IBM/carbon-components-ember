{{import ListRow from '../-row'}}
{{import '{ hash }' from '@ember/helper'}}

<tbody>
  {{#each @items as |item|}}
    {{yield
      (hash
        Row=(component
          ListRow
          isExpandable=@isExpandable
          isCheckable=@isCheckable
          table=@table
          item=item
        )
        item=item
      )
    }}
  {{/each}}
</tbody>